{
  "name": "runty-logger",
  "version": "1.1.1",
  "description": "Lightweight unoriginal logger",
  "license": "MIT",
  "main": "src/index.js",
  "browser": "browser/runtylogger.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiaanduplessis/runty-logger.git"
  },
  "homepage": "https://github.com/tiaanduplessis/runty-logger",
  "bugs": "https://github.com/tiaanduplessis/runty-logger/issues",
  "author": {
    "name": "Tiaan",
    "email": "tiaanduplessis@hotmail.com",
    "url": "tiaanduplessis.co.za"
  },
  "scripts": {
    "start": "npm run build; npm run build:min",
    "build": "webpack",
    "build:min": "WEBPACK_ENV=production webpack -p",
    "pretest": "npm run lint",
    "test": "jest --env=node",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "precommit": "npm test"
  },
  "keywords": [
    "logger",
    "log",
    "logging",
    "console",
    "console.log",
    "runty-logger"
  ],
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.3.3",
    "husky": "^0.14.3",
    "jest": "^22.0.0",
    "standard": "^11.0.0",
    "webpack": "^4.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "standard": {
    "ignore": [
      "browser/**"
    ]
  }
}
